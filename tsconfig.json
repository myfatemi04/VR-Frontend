{
  "compilerOptions": {
    "allowJs": true,
    "module": "CommonJS",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "preserve",
    "target": "es5",
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules/"
  ],
  "include": [
    "next-env.d.ts",
    "index.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
